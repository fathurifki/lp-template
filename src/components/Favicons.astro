---
import favIcon from '~/assets/favicons2/favicon.ico';
import favIconPng from '~/assets/favicons2/favicon.png';
import appleTouchIcon from '~/assets/favicons2/apple-touch-icon.png';
---

<link rel="shortcut icon" href={favIcon} />
<link rel="icon" type="image/svg+xml" href={favIconPng.src} />
<link rel="mask-icon" href={favIconPng.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
